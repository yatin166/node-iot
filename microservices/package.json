{
  "name": "microservices",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:admin:dev": "node -r dotenv/config ./build/admin/src/index.js",
    "start:authenticator:dev": "node -r dotenv/config ./build/authenticator/src/index.js",
    "start:dashboard:dev": "node -r dotenv/config ./build/dashboard/src/index.js",
    "start:dev": "npm run start:authenticator:dev & npm run start:admin:dev & npm run start:dashboard:dev",
    "start:admin:prod": "npm run tsc && node -r dotenv/config ./build/admin/src/index.js",
    "start:prod": "ENVIRONMENT=prod npm run start:admin:prod",
    "start:db": "docker run -d --name=mongo -it -p 27017:27017 mongo"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^15.14.2",
    "@types/socket.io": "^3.0.2",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.14",
    "nodemon": "^2.0.7",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
